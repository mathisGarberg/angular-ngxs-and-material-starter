{
  "name": "angular-material-starter",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 50000",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng lint && ng test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "lint": "ng lint",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/angular-ngrx-material-starter/stats-es2015.json",
    "e2e": "run-p --race e2e:runtests e2e:runserver",
    "e2e:runtests": "cypress run --reporter teamcity",
    "e2e:runserver": "http-server ../dist -a localhost -p 50000 -silent -c-1",
    "e2e: dev": "start ng serve --port 50000 && start cypress open",
    "hmr": "ng serve --configuration hmr --port 50000",
    "cypress": "concurrently \"ng serve\" \"cypress open\"",
    "e2e:dev": "start ng serve --port 50000 && start cypress open"
  },
  "husky": {
    "hooks": {
      "pre-push": "pretty-quick --staged && ng lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@markpieszak/ng-application-insights": "^8.0.1",
    "@ngxs/devtools-plugin": "^3.6.1",
    "@ngxs/form-plugin": "^3.6.2",
    "@ngxs/hmr-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@sentry/browser": "^5.15.4",
    "ajv": "^6.11.0",
    "browser-detect": "^0.2.28",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "http-server": "^0.12.1",
    "ngx-bar-rating": "^1.1.0",
    "promise-polyfill": "8.1.3",
    "rxjs": "~6.5.5",
    "subsink": "^1.0.1",
    "tslib": "^1.11.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.26",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "~9.0.5",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.1.0",
    "cypress": "^4.0.2",
    "cypress": "^4.3.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.3",
    "tslint-teamcity-reporter": "^3.2.2",
    "typescript": "^3.5.3"
  }
}
